        <!--提升额度-->
        <ion-col col-12>
            <div class="circle">
                <ul>
                    <h3>我的额度(元)</h3>
                    <span class="limit">{{qmoney}}</span>
                    <p *ngIf='zore' tappable (click)='getQmoney()'>重新生成额度></p>
                    <p *ngIf='!zore'></p>
                </ul>
            </div>
        </ion-col>

