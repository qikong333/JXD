            <i class="icon iconfont icon-gengduo"></i>
            <p style="margin: 0;">{{list4}}</p>
        </dl>
    </ion-row>
    <ion-row class="box">
        <dl tappable (click)="list5State(list5)" *ngIf="isDisabled(list4)">
