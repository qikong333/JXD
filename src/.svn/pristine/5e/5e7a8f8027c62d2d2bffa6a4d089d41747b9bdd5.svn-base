@import '../../theme/base.scss';
page-novice {
.list_box{
  width:90%;
  margin: 0 5%;
  float: left;
  li{
    width: 100%;
    float: left;
    margin-top: r(30px);
  }
  span{
    width: 100%;
    float: left;
    color: #323232;
    font-size:r(26px);
  }
  p{
    width: 100%;
    float: left;
    color: #646464;
    font-size:r(22px);
    text-align: justify;
    text-indent: 2em;
  }
  i{
    float:left;
    font-size: r(26px);
    color: #ffb710;
  }
}
  .disnone{
    display: none;
  }
  .disblock{
    display: block;
  }
}
