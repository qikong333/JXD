page-loan-means {
    .loan_means{
        width:100%;
        height:100%;
        background:#ffffff;
        /*padding:0 5% 0 5%;*/
        .hear_data{
            width:100%;
            padding:0 5% 0 5%;
            li{
                width:50%;
                background:#fff;
                float:left;
                border-bottom:r(1px )solid #ccc;
                .header_to{
                    /*height:auto;*/
                    width:100%;
                    float:left;
                    span{
                        float:left;
                        font-size:r(26px);
                        color:#323232;
                        margin-top:r(30px);
                    }
                    i{
                        float:right;
                        margin-top:r(20px);
                        color:#ffb710;
                        font-size:r(40px);
                        margin-right:r(30px);
                    }
                }
                .content{
                    width:100%;
                    font-size:r(22px);
                    color:#909090;
                }
                .bottom{
                    width:100%;
                    font-size:r(22px);
                    color:#909090;
                    margin-bottom:r(30px);
                }
            }
            li:nth-child(3){
                border:none;
            }
            li:nth-child(2n){
                width:50%;
                padding-left:r(30px);
            }
        }
    
        /*li:after{
            content:'';
            width:1px;
            height:1px;
            background:#ccc;
        }*/
        .bank{
            width:100%;
            height:r(20px);
            background:#ededed;
        }
        .main li{
            width:100%;
            padding:0 5% 0 5%;
            border-bottom:r(1px) solid #ccc;
            .title_1{
                font-size:r(26px);
                color:#323232;
                margin-top:r(20px);
                float:left;
            }
            .article_1{
                float:right;
                font-size:r(45px);
                color:#ffb710;
                margin-top:r(10px);
                text-align:right;
            }
            .article_2{
                width:100%;
                float:left;
                font-size:r(20px);
                color:#909090;
                margin-bottom:r(20px);
                text-align:right;
            }
        }

        .clear{
            clear:both;
        }
    }
}
