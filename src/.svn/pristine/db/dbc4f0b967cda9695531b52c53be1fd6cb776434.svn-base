page-coupons {
    .coupons{
        background:#ededed;
    }
    .coupons_all{
        width:100%;
        height:r(75px);
        line-height:r(75px);
        border-bottom:1px solid #ededed;
        background:#ffffff;
    }
    .coupons_all .coupons_gg{
        font-size:r(24px);
        height:r(75px);
        line-height:r(75px);
        color:#666666;
        text-align:center;
        border:0;
    }
    .segment-ios .segment-button.segment-activated{
        background:#ffffff;
        color:#eeb74a;
        border-bottom:1px solid #eeb74a;
    }

    //content内容
    .coupons_volume{
         width:90.625%;
         margin:0 auto;
         padding-top:r(30px);
     }
     .coupons_volume li{
         margin-top:r(30px);
     }
     .coupons_volume li:nth-child(1){
         margin-top:0;
     }
     .coupons_volume .coupons_volume_free{
         width:100%;
         height:r(200px);
         background:url("../assets/images/coupons_blue.png") no-repeat;
         background-size:100% 100%;
         position:relative;
     }
     .coupons_volume .coupons_volume_mortgage{
         width:100%;
         height:r(200px);
         background:url("../assets/images/coupons_pink.png") no-repeat;
         background-size:100% 100%;
         position:relative;
     }
     .coupons_volume_left{
         float:left;
         width:39.65%;
     }
     .coupons_volume_center{
         width:60.35%;
         float:left;
         font-size:r(24px);
         color:#ffffff;
     }
     .coupons_volume_left .coupons_volume_header{
         font-size:r(24px);
         color:#ffffff;
         margin:r(20px) 0 r(0px) r(20px);
     }
      .coupons_volume_left .coupons_volume_header::first-letter{
          font-size:r(36px);
             color:#ffffff;
     }

     .coupons_volume_left .coupons_volume_title{
         text-align:center;
         font-size:r(30px);
         color:#ffffff;
         height:r(80px);
         line-height:r(80px);
     }
      .coupons_volume_left .coupons_volume_title span{
         font-size:r(60px);
         color:#ffffff;
         font-family: "SourceHanSansCN-Bold"
     }

    
     .coupons_volume_left .coupons_volume_time{
         text-align:center;
         color:#ffffff;
         margin-top:r(10px);
         font-size:r(20px);
     }
     
     .coupons_volume_center .coupons_volume_use{
         margin-left:r(30px);
         margin-top:r(70px);
     }
     .coupons_volume_center .coupons_volume_liability{
        margin-left:r(30px);
     }

     .coupons_volume_right{
         width:25%;
        position:absolute;
        top:0;
        right:0;
     }
      .coupons_volume_right3{
         width:25%;
        position:absolute;
        top:0;
        right:0;
     }

    //效果--滤镜和透明
    .coupons_first{
        background:#ffffff;
    }
    .coupons_two{
        opacity:0.7;
        background:#ffffff;
    }
    .coupons_three{
       filter:grayscale(100%);
       opacity:0.7;
       background:#ffffff;
    }
    
    /*没有更多数据*/
    .nodata{
        height:r(84px);
        line-height:r(84px);
        text-align:center;
        font-size:r(26px);
        color:#909090;
        font-family: "华文细黑"
    }

    /*无数据*/
    .coupons_volume1{
         width:100%;
         height:100%;
        background:#ededed;
        text-align:center;
        font-size:r(26px);
        color:#909090;
        margin-top:r(355px);
    }
    .coupons_volume2{
         width:100%;
         height:100%;
         background:#ededed;
        text-align:center;
        font-size:r(26px);
        color:#909090;
        margin-top:r(355px);
    }
     .coupons_volume3{
         width:100%;
         height:100%;
         background:#ededed;
        text-align:center;
        font-size:r(26px);
        color:#909090;
        margin-top:r(355px);
    }

    img{
        display:block;
    }













}
